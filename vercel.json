{
  "version": 2,
  "name": "business-mechanicaleng",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/dist/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "noindex"
        }
      ]
    }
  ]
}
